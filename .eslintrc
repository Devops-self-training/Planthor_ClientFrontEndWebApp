{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "react-app",
        "Prettier"
    ],
    "ignorePatterns": ["build/*"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "complexity": ["error", 22],
        "camelcase": "error",
        "prettier/prettier": [
            "warn",
            {
                "arrowParens": "avoid",
                "semi": true,
                "trailingComma": "none",
                "endOfLine": "lf",
                "tabWidth": 4,
                "printWidth": 80,
                "useTabs": false
            }
        ],
        "no-console": "warn"
    }
}
